<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
    <channel>
        <title>Flux URSS - Tsarbucks</title>
        <link>https://www.tsarbucks.com</link>
        <description>Actualités impériales de Tsarbucks</description>
        <language>fr</language>

        {% for item in news %}
            <item>
                <title>{{ item.title }}</title>
                <link>{{ item.link }}</link>
                <description>{{ item.description }}</description>
                <pubDate>{{ item.pubDate }}</pubDate>
            </item>
        {% endfor %}
    </channel>
</rss>
